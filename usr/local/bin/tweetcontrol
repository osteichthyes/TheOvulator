#!/bin/sh

USERNAME="@YOUR_USERNAME"

while [ 1==1 ]
do
  CC=/bin/twidge -c $HOME/.twidgerc -s -u LSDM | grep $USERNAME | grep "OVULATE" -c
  if [ CC>=1 ]
  then
    twidge -c $HOME/.twidgerc update "Forcing an ovulation $USERNAME"
    /bin/python3 /usr/local/bin/servo.py\
    /bin/sleep 300
  else
    /bin/sleep 300
  fi
done
